/// Calculation table containing 112 DAX measures organized into folders: Parameters (00), Core KPIs (01), Engagement (02), Distribution (03), Channels (04), Time Intelligence (05), Ranking (06), KPI Indicators (07), Data Quality (08), Cumulative (09), Performance Context (10), Day Analysis (11), and Summary (12). All measures include descriptions.
table Metrics
	lineageTag: pr-metrics-001

	/// Total number of page views across all press room pages. Includes both landing pages and press releases.
	measure 'Total Views' = SUM(Fact_Press_Analytics[Views])
		formatString: #,0
		displayFolder: 01 - Core KPIs
		lineageTag: pr-m-totalviews

	/// Total number of unique users who visited the press room pages.
	measure 'Total Users' = SUM(Fact_Press_Analytics[Users])
		formatString: #,0
		displayFolder: 01 - Core KPIs
		lineageTag: pr-m-totalusers

		annotation PBI_FormatHint = {"isDecimal":true}

	/// Total number of user sessions on press room pages.
	measure 'Total Sessions' = SUM(Fact_Press_Analytics[Sessions])
		formatString: #,0
		displayFolder: 01 - Core KPIs
		lineageTag: pr-m-totalsessions

	/// Total page views for the press room landing page.
	measure 'Landing Page Views' = CALCULATE(SUM(Fact_Press_Analytics[Views]), Fact_Press_Analytics[Page_Type] = "Landing Page")
		formatString: #,0
		displayFolder: 01 - Core KPIs
		lineageTag: pr-m-landingviews

	/// Total page views for all press release pages.
	measure 'Press Release Views' = ```
			
			SUM(PressRelease_Summary[Total Views])
			
			```
		formatString: #,0
		displayFolder: 01 - Core KPIs
		lineageTag: pr-m-releaseviews

	/// Count of distinct press releases in the dataset.
	measure '# Press Releases' = CALCULATE(DISTINCTCOUNT(Fact_Press_Analytics[Page_URL]), Fact_Press_Analytics[Page_Type] = "Press Release")
		formatString: #,0
		displayFolder: 01 - Core KPIs
		lineageTag: pr-m-releasecount

	/// Average time users spent on press room pages, in seconds. NOTE: Currently returns 0 as GA4 export does not include engagement time metrics. Placeholder for future data integration.
	measure 'Avg Time on Page' = 0
		formatString: 0.0
		isHidden
		displayFolder: 02 - Engagement
		lineageTag: pr-m-avgtime

	/// Average bounce rate percentage for press room pages. NOTE: Currently returns 0 as GA4 export does not include bounce rate metrics. Placeholder for future data integration.
	measure 'Avg Bounce Rate' = 0
		formatString: 0.0%
		isHidden
		displayFolder: 02 - Engagement
		lineageTag: pr-m-bouncerate

	/// Total number of page entrances (first page in a session). NOTE: Currently returns 0 as GA4 export does not include entrance metrics. Placeholder for future data integration.
	measure 'Total Entrances' = 0
		formatString: #,0
		isHidden
		displayFolder: 02 - Engagement
		lineageTag: pr-m-entrances

	/// Total number of page exits (last page in a session). NOTE: Currently returns 0 as GA4 export does not include exit metrics. Placeholder for future data integration.
	measure 'Total Exits' = 0
		formatString: #,0
		isHidden
		displayFolder: 02 - Engagement
		lineageTag: pr-m-exits

	/// Count of new users (first-time visitors) to press room pages. NOTE: GA4 export does not distinguish new vs returning users - this currently equals Total Users. Placeholder for future enhanced data.
	measure 'New Users' = SUM(Fact_Press_Analytics[Users])
		formatString: #,0
		isHidden
		displayFolder: 02 - Engagement
		lineageTag: pr-m-newusers

	/// Percentage of users who are new (first-time visitors). NOTE: GA4 export does not distinguish new vs returning users - this will always show 100%. Placeholder for future enhanced data.
	measure 'New User Rate' = DIVIDE([New Users], [Total Users], 0)
		formatString: 0.0%
		isHidden
		displayFolder: 02 - Engagement
		lineageTag: pr-m-newuserrate

	/// Average number of page views per unique user.
	measure 'Views per User' = DIVIDE([Total Views], [Total Users], 0)
		formatString: 0.0
		displayFolder: 02 - Engagement
		lineageTag: pr-m-viewsperuser

	/// Average number of sessions per unique user.
	measure 'Sessions per User' = DIVIDE([Total Sessions], [Total Users], 0)
		formatString: 0.0
		displayFolder: 02 - Engagement
		lineageTag: pr-m-sessionsperuser

	/// Percentage of total views that are landing page views.
	measure 'Landing Page Share %' = DIVIDE([Landing Page Views], [Total Views], 0)
		formatString: 0.0%
		displayFolder: 03 - Distribution
		lineageTag: pr-m-landingshare

	/// Percentage of total views that are press release views.
	measure 'Press Release Share %' = DIVIDE([Press Release Views], [Total Views], 0)
		formatString: 0.0%
		displayFolder: 03 - Distribution
		lineageTag: pr-m-releaseshare

	/// Average number of views per press release.
	measure 'Avg Views per Release' = DIVIDE([Press Release Views], [# Press Releases], 0)
		formatString: #,0
		displayFolder: 03 - Distribution
		lineageTag: pr-m-avgviewsperrelease

	/// Percentage of total views for the current context.
	measure '% of Total Views' = DIVIDE([Total Views], CALCULATE([Total Views], REMOVEFILTERS()), 0)
		formatString: 0.0%
		displayFolder: 03 - Distribution
		lineageTag: pr-m-pctoftotalviews

	/// Percentage of parent category views.
	measure '% of Parent Views' = DIVIDE([Total Views], CALCULATE([Total Views], REMOVEFILTERS(Fact_Press_Analytics[Page_URL])), 0)
		formatString: 0.0%
		displayFolder: 03 - Distribution
		lineageTag: pr-m-pctofparentviews

	/// Total page views from organic search traffic.
	measure 'Organic Search Views' = CALCULATE(SUM(Fact_Press_Analytics[Views]), Fact_Press_Analytics[Channel_Group] = "Organic Search")
		formatString: #,0
		displayFolder: 04 - Channels
		lineageTag: pr-m-organicviews

	/// Total page views from direct traffic (no referrer).
	measure 'Direct Views' = CALCULATE(SUM(Fact_Press_Analytics[Views]), Fact_Press_Analytics[Channel_Group] = "Direct")
		formatString: #,0
		displayFolder: 04 - Channels
		lineageTag: pr-m-directviews

	/// Total page views from referral traffic (external websites).
	measure 'Referral Views' = CALCULATE(SUM(Fact_Press_Analytics[Views]), Fact_Press_Analytics[Channel_Group] = "Referral")
		formatString: #,0
		displayFolder: 04 - Channels
		lineageTag: pr-m-referralviews

	/// Total page views from social media traffic.
	measure 'Social Views' = CALCULATE(SUM(Fact_Press_Analytics[Views]), Fact_Press_Analytics[Channel_Group] IN {"Organic Social", "Social"})
		formatString: #,0
		displayFolder: 04 - Channels
		lineageTag: pr-m-socialviews

	/// Total page views from email campaigns.
	measure 'Email Views' = CALCULATE(SUM(Fact_Press_Analytics[Views]), Fact_Press_Analytics[Channel_Group] = "Email")
		formatString: #,0
		displayFolder: 04 - Channels
		lineageTag: pr-m-emailviews

	/// Percentage of total views from organic search.
	measure 'Organic Search %' = DIVIDE([Organic Search Views], [Total Views], 0)
		formatString: 0.0%
		displayFolder: 04 - Channels
		lineageTag: pr-m-organicpct

	/// Percentage of total views from direct traffic.
	measure 'Direct %' = DIVIDE([Direct Views], [Total Views], 0)
		formatString: 0.0%
		displayFolder: 04 - Channels
		lineageTag: pr-m-directpct

	/// Percentage of total views from referral traffic.
	measure 'Referral %' = DIVIDE([Referral Views], [Total Views], 0)
		formatString: 0.0%
		displayFolder: 04 - Channels
		lineageTag: pr-m-referralpct

	/// Percentage of total views from social media.
	measure 'Social %' = DIVIDE([Social Views], [Total Views], 0)
		formatString: 0.0%
		displayFolder: 04 - Channels
		lineageTag: pr-m-socialpct

	/// Percentage of total views from email campaigns.
	measure 'Email %' = DIVIDE([Email Views], [Total Views], 0)
		formatString: 0.0%
		displayFolder: 04 - Channels
		lineageTag: pr-m-emailpct

	/// Year-to-date total page views from the start of the current year.
	measure 'Views YTD' = CALCULATE([Total Views], DATESYTD(Dim_Date[Date]))
		formatString: #,0
		displayFolder: 05 - Time Intelligence
		lineageTag: pr-m-viewsytd

	/// Same period last year - total page views for the equivalent period in the previous year.
	measure 'Views SPLY' = CALCULATE([Total Views], SAMEPERIODLASTYEAR(Dim_Date[Date]))
		formatString: #,0
		displayFolder: 05 - Time Intelligence
		lineageTag: pr-m-viewsyoy

	/// Year-over-year percentage change in page views compared to the same period last year.
	measure 'Views YoY %' =
			VAR CurrentPeriod = [Total Views]
			VAR PriorPeriod = [Views SPLY]
			RETURN
			DIVIDE(CurrentPeriod - PriorPeriod, PriorPeriod, BLANK())
		formatString: +0.0%;-0.0%;0.0%
		displayFolder: 05 - Time Intelligence
		lineageTag: pr-m-viewsyoypct

	/// Month-to-date total page views from the start of the current month.
	measure 'Views MTD' = CALCULATE([Total Views], DATESMTD(Dim_Date[Date]))
		formatString: #,0
		displayFolder: 05 - Time Intelligence
		lineageTag: pr-m-viewsmtd

	/// Previous month total page views.
	measure 'Views PM' = CALCULATE([Total Views], DATEADD(Dim_Date[Date], -1, MONTH))
		formatString: #,0
		displayFolder: 05 - Time Intelligence
		lineageTag: pr-m-viewspm

	/// Month-over-month percentage change in page views.
	measure 'Views MoM %' =
			VAR CurrentPeriod = [Total Views]
			VAR PriorPeriod = [Views PM]
			RETURN
			DIVIDE(CurrentPeriod - PriorPeriod, PriorPeriod, BLANK())
		formatString: +0.0%;-0.0%;0.0%
		displayFolder: 05 - Time Intelligence
		lineageTag: pr-m-viewsmompct

	/// Cumulative running total respecting filter context (uses ALLSELECTED). Responds to slicers and report filters.
	measure 'Running Total Views' =
			CALCULATE(
			    [Total Views],
			    FILTER(
			        ALLSELECTED(Dim_Date[Date]),
			        Dim_Date[Date] <= MAX(Dim_Date[Date])
			    )
			)
		formatString: #,0
		displayFolder: 09 - Cumulative
		lineageTag: pr-m-runningtotalviews

	/// 7-day moving average of daily page views.
	measure 'Views 7-Day Avg' =
			AVERAGEX(
			    DATESINPERIOD(Dim_Date[Date], MAX(Dim_Date[Date]), -7, DAY),
			    CALCULATE([Total Views])
			)
		formatString: #,0
		displayFolder: 05 - Time Intelligence
		lineageTag: pr-m-views7dayavg

	/// 30-day moving average of daily page views.
	measure 'Views 30-Day Avg' =
			AVERAGEX(
			    DATESINPERIOD(Dim_Date[Date], MAX(Dim_Date[Date]), -30, DAY),
			    CALCULATE([Total Views])
			)
		formatString: #,0
		displayFolder: 05 - Time Intelligence
		lineageTag: pr-m-views30dayavg

	/// Ranking of pages by total views (1 = highest views).
	measure 'View Rank' = RANKX(ALL(Fact_Press_Analytics[Page_URL]), CALCULATE(SUM(Fact_Press_Analytics[Views])), , DESC, DENSE)
		formatString: 0
		displayFolder: 06 - Ranking
		lineageTag: pr-m-viewrank

	/// Boolean indicator (1 or 0) if the page is in the top 10 by views.
	measure 'Is Top 10' = IF([View Rank] <= 10, "Yes", "No")
		isHidden
		displayFolder: 06 - Ranking
		lineageTag: pr-m-istop10

	/// Boolean indicator (1 or 0) if the page is in the top 5 by views.
	measure 'Is Top 5' = IF([View Rank] <= 5, "Yes", "No")
		isHidden
		displayFolder: 06 - Ranking
		lineageTag: pr-m-istop5

	/// Title of the press release with the highest views.
	measure 'Top Press Release' =
			VAR TopPage =
			    TOPN(
			        1,
			        CALCULATETABLE(
			            SUMMARIZE(
			                FILTER(
			                    Fact_Press_Analytics,
			                    Fact_Press_Analytics[Page_Type] = "Press Release" &&
			                    Fact_Press_Analytics[Page_URL] IN VALUES(Dim_Press_Releases[Page_URL])
			                ),
			                Fact_Press_Analytics[Page_Title],
			                "TotalViews", SUM(Fact_Press_Analytics[Views])
			            ),
			            Fact_Press_Analytics[Page_Type] = "Press Release",
			            Fact_Press_Analytics[Page_URL] IN VALUES(Dim_Press_Releases[Page_URL])
			        ),
			        [TotalViews],
			        DESC
			    )
			RETURN
			MAXX(TopPage, Fact_Press_Analytics[Page_Title])
		displayFolder: 06 - Ranking
		lineageTag: pr-m-toppressrelease

	/// Total views for the highest-performing press release.
	measure 'Top Release Views' =
			VAR TopViews =
			    MAXX(
			        CALCULATETABLE(
			            SUMMARIZE(
			                FILTER(
			                    Fact_Press_Analytics,
			                    Fact_Press_Analytics[Page_Type] = "Press Release" &&
			                    Fact_Press_Analytics[Page_URL] IN VALUES(Dim_Press_Releases[Page_URL])
			                ),
			                Fact_Press_Analytics[Page_URL],
			                "TotalViews", SUM(Fact_Press_Analytics[Views])
			            ),
			            Fact_Press_Analytics[Page_Type] = "Press Release",
			            Fact_Press_Analytics[Page_URL] IN VALUES(Dim_Press_Releases[Page_URL])
			        ),
			        [TotalViews]
			    )
			RETURN TopViews
		formatString: #,0
		displayFolder: 06 - Ranking
		lineageTag: pr-m-toppressreleaseviews

	/// Ranking of channels by total views (1 = highest views).
	measure 'Channel Rank' = RANKX(ALL(Fact_Press_Analytics[Channel_Group]), CALCULATE([Total Views]), , DESC, DENSE)
		formatString: 0
		displayFolder: 06 - Ranking
		lineageTag: pr-m-channelrank

	/// Indicator value for year-over-year comparison visualization.
	measure 'YoY Indicator' =
			VAR Change = [Views YoY %]
			RETURN
			SWITCH(
			    TRUE(),
			    Change > 0.05, 1,
			    Change < -0.05, -1,
			    0
			)
		formatString: 0
		isHidden
		displayFolder: 07 - KPI Indicators
		lineageTag: pr-m-yoyindicator

	/// Indicator value for month-over-month comparison visualization.
	measure 'MoM Indicator' =
			VAR Change = [Views MoM %]
			RETURN
			SWITCH(
			    TRUE(),
			    Change > 0.05, 1,
			    Change < -0.05, -1,
			    0
			)
		formatString: 0
		isHidden
		displayFolder: 07 - KPI Indicators
		lineageTag: pr-m-momindicator

	/// Arrow direction indicator for year-over-year trend (up/down/neutral).
	measure 'YoY Arrow' =
			VAR Change = [Views YoY %]
			RETURN
			SWITCH(
			    TRUE(),
			    Change > 0.05, "▲",
			    Change < -0.05, "▼",
			    "●"
			)
		displayFolder: 07 - KPI Indicators
		lineageTag: pr-m-yoyarrow

	/// Color indicator for year-over-year performance (green/red/neutral).
	measure 'YoY Color' =
			VAR Change = [Views YoY %]
			RETURN
			SWITCH(
			    TRUE(),
			    Change > 0.05, "#2E8540",
			    Change < -0.05, "#D54309",
			    "#71767A"
			)
		isHidden
		displayFolder: 07 - KPI Indicators
		lineageTag: pr-m-yoycolor

	/// Total number of data rows in the press room dataset.
	measure 'Row Count' = COUNTROWS(Fact_Press_Analytics)
		formatString: #,0
		displayFolder: 08 - Data Quality
		lineageTag: pr-m-rowcount

	/// Count of distinct page paths in the dataset.
	measure '# Distinct Pages' = DISTINCTCOUNT(Fact_Press_Analytics[Page_URL])
		formatString: #,0
		displayFolder: 08 - Data Quality
		lineageTag: pr-m-distinctpages

	/// Count of distinct traffic channels in the dataset.
	measure '# Channels' = DISTINCTCOUNT(Fact_Press_Analytics[Channel_Group])
		formatString: #,0
		displayFolder: 08 - Data Quality
		lineageTag: pr-m-distinctchannels

	/// Earliest date in the dataset.
	measure 'Data Start Date' = MIN(Fact_Press_Analytics[Date])
		formatString: Short Date
		displayFolder: 08 - Data Quality
		lineageTag: pr-m-datastartdate

	/// Latest date in the dataset.
	measure 'Data End Date' = MAX(Fact_Press_Analytics[Date])
		formatString: Short Date
		displayFolder: 08 - Data Quality
		lineageTag: pr-m-dataenddate

	/// Formatted date range string for display (e.g., 'March 11 2025 - December 15 2025').
	measure 'Date Range Label' = "Data: " & FORMAT([Data Start Date], "MMM D, YYYY") & " – " & FORMAT([Data End Date], "MMM D, YYYY")
		displayFolder: 08 - Data Quality
		lineageTag: pr-m-daterangelabel

	/// DEPRECATED: Use 'Top 10 PR Views' instead. This old measure is kept for backward compatibility but should not be used in new visuals.
	/// Total views for the top 10 press releases by views.
	measure 'Top 10 Press Release Views' = [Top 10 PR Views]
		formatString: #,0
		displayFolder: 03 - Distribution
		lineageTag: pr-m-top10releaseviews
		isHidden: true

	/// DEPRECATED: Use 'Top 10 Share' instead. This old measure is kept for backward compatibility but should not be used in new visuals.
	/// Percentage of press release traffic from top 10 releases.
	measure 'Top 10 % of Press Release Traffic' = [Top 10 Share]
		formatString: 0.0%
		displayFolder: 03 - Distribution
		lineageTag: pr-m-top10pct
		isHidden: true

	/// DEPRECATED: Use 'Remaining PR Views' instead. This old measure is kept for backward compatibility but should not be used in new visuals.
	/// Total views for press releases not in the top 10.
	measure 'Remaining Press Release Views' = [Remaining PR Views]
		formatString: #,0
		displayFolder: 03 - Distribution
		lineageTag: pr-m-remainingreleaseviews
		isHidden: true

	/// DEPRECATED: This measure is kept for backward compatibility but should not be used in new visuals.
	/// Percentage of press release traffic from releases outside top 10.
	measure 'Remaining Press Release %' = DIVIDE([Remaining PR Views], [Press Release Views], 0)
		formatString: 0.0%
		displayFolder: 03 - Distribution
		lineageTag: pr-m-remainingpct
		isHidden: true

	/// Organic search views for press release pages only.
	measure 'Organic Search Views (Press Releases Only)' =
			CALCULATE(
			    SUM(Fact_Press_Analytics[Views]),
			    Fact_Press_Analytics[Channel_Group] = "Organic Search",
			    Fact_Press_Analytics[Page_Type] = "Press Release"
			)
		formatString: #,0
		displayFolder: 04 - Channels
		lineageTag: pr-m-organicviewspr

	/// Direct traffic views for press release pages only.
	measure 'Direct Views (Press Releases Only)' =
			CALCULATE(
			    SUM(Fact_Press_Analytics[Views]),
			    Fact_Press_Analytics[Channel_Group] = "Direct",
			    Fact_Press_Analytics[Page_Type] = "Press Release"
			)
		formatString: #,0
		displayFolder: 04 - Channels
		lineageTag: pr-m-directviewspr

	/// Referral traffic views for press release pages only.
	measure 'Referral Views (Press Releases Only)' =
			CALCULATE(
			    SUM(Fact_Press_Analytics[Views]),
			    Fact_Press_Analytics[Channel_Group] = "Referral",
			    Fact_Press_Analytics[Page_Type] = "Press Release"
			)
		formatString: #,0
		displayFolder: 04 - Channels
		lineageTag: pr-m-referralviewspr

	/// Social media views for press release pages only.
	measure 'Social Views (Press Releases Only)' =
			CALCULATE(
			    SUM(Fact_Press_Analytics[Views]),
			    Fact_Press_Analytics[Channel_Group] = "Organic Social",
			    Fact_Press_Analytics[Page_Type] = "Press Release"
			)
		formatString: #,0
		displayFolder: 04 - Channels
		lineageTag: pr-m-socialviewspr

	/// Email campaign views for press release pages only.
	measure 'Email Views (Press Releases Only)' =
			CALCULATE(
			    SUM(Fact_Press_Analytics[Views]),
			    Fact_Press_Analytics[Channel_Group] = "Email",
			    Fact_Press_Analytics[Page_Type] = "Press Release",
			    Fact_Press_Analytics[Page_URL] IN VALUES(Dim_Press_Releases[Page_URL])
			)
		formatString: #,0
		displayFolder: 04 - Channels
		lineageTag: pr-m-emailviewspr

	/// Percentage of press release views from organic search.
	measure 'Organic Search % (Press Releases Only)' = DIVIDE([Organic Search Views (Press Releases Only)], [Press Release Views], 0)
		formatString: 0.0%
		displayFolder: 04 - Channels
		lineageTag: pr-m-organicpctpr

	/// Percentage of press release views from direct traffic.
	measure 'Direct % (Press Releases Only)' = DIVIDE([Direct Views (Press Releases Only)], [Press Release Views], 0)
		formatString: 0.0%
		displayFolder: 04 - Channels
		lineageTag: pr-m-directpctpr

	/// Percentage of press release views from referral traffic.
	measure 'Referral % (Press Releases Only)' = DIVIDE([Referral Views (Press Releases Only)], [Press Release Views], 0)
		formatString: 0.0%
		displayFolder: 04 - Channels
		lineageTag: pr-m-referralpctpr

	/// Percentage of press release views from social media.
	measure 'Social % (Press Releases Only)' = DIVIDE([Social Views (Press Releases Only)], [Press Release Views], 0)
		formatString: 0.0%
		displayFolder: 04 - Channels
		lineageTag: pr-m-socialpctpr

	/// Percentage of press release views from email campaigns.
	measure 'Email % (Press Releases Only)' = DIVIDE([Email Views (Press Releases Only)], [Press Release Views], 0)
		formatString: 0.0%
		displayFolder: 04 - Channels
		lineageTag: pr-m-emailpctpr

	/// Total page views for the current week (Sunday through Saturday).
	measure 'Views This Week' =
			VAR MaxDate = MAX(Dim_Date[Date])
			VAR WeekStart = MaxDate - WEEKDAY(MaxDate, 2) + 1
			RETURN
			CALCULATE(
			    [Total Views],
			    Dim_Date[Date] >= WeekStart && Dim_Date[Date] <= MaxDate
			)
		formatString: #,0
		displayFolder: 05 - Time Intelligence
		lineageTag: pr-m-viewsthisweek

	/// Total page views for the previous week (Sunday through Saturday).
	measure 'Views Last Week' =
			VAR MaxDate = MAX(Dim_Date[Date])
			VAR LastWeekEnd = MaxDate - WEEKDAY(MaxDate, 2)
			VAR LastWeekStart = LastWeekEnd - 6
			RETURN
			CALCULATE(
			    [Total Views],
			    Dim_Date[Date] >= LastWeekStart && Dim_Date[Date] <= LastWeekEnd
			)
		formatString: #,0
		displayFolder: 05 - Time Intelligence
		lineageTag: pr-m-viewslastweek

	/// Week-over-week percentage change in page views.
	measure 'Views WoW %' =
			VAR CurrentWeek = [Views This Week]
			VAR PriorWeek = [Views Last Week]
			RETURN
			DIVIDE(CurrentWeek - PriorWeek, PriorWeek, BLANK())
		formatString: +0.0%;-0.0%;0.0%
		displayFolder: 05 - Time Intelligence
		lineageTag: pr-m-viewswowpct

	/// Year-to-date total unique users from the start of the current year.
	measure 'Users YTD' = CALCULATE([Total Users], DATESYTD(Dim_Date[Date]))
		formatString: #,0
		displayFolder: 05 - Time Intelligence
		lineageTag: pr-m-usersytd

	/// Year-to-date total sessions from the start of the current year.
	measure 'Sessions YTD' = CALCULATE([Total Sessions], DATESYTD(Dim_Date[Date]))
		formatString: #,0
		displayFolder: 05 - Time Intelligence
		lineageTag: pr-m-sessionsytd

	/// Year-to-date count of press releases.
	measure 'Press Releases YTD' =
			CALCULATE(
			    [# Press Releases],
			    DATESYTD(Dim_Date[Date])
			)
		formatString: #,0
		displayFolder: 05 - Time Intelligence
		lineageTag: pr-m-releasesytd

	/// Formatted label for the current reporting week.
	measure 'Report Week Label' =
			VAR MaxDate = MAX(Dim_Date[Date])
			VAR WeekStart = MaxDate - WEEKDAY(MaxDate, 2) + 1
			RETURN
			"Week of " & FORMAT(WeekStart, "MMM D") & " - " & FORMAT(MaxDate, "MMM D, YYYY")
		displayFolder: 08 - Data Quality
		lineageTag: pr-m-reportweeklabel

	/// Formatted label for year-to-date period.
	measure 'YTD Label' = "Year-to-Date: Jan 1 - " & FORMAT(MAX(Dim_Date[Date]), "MMM D, YYYY")
		displayFolder: 08 - Data Quality
		lineageTag: pr-m-ytdlabel

	/// Arrow direction indicator for week-over-week trend (up/down/neutral).
	measure 'WoW Arrow' =
			VAR Change = [Views WoW %]
			RETURN
			SWITCH(
			    TRUE(),
			    Change > 0.05, "▲",
			    Change < -0.05, "▼",
			    "●"
			)
		displayFolder: 07 - KPI Indicators
		lineageTag: pr-m-wowarrow

	/// Color indicator for week-over-week performance (green/red/neutral).
	measure 'WoW Color' =
			VAR Change = [Views WoW %]
			RETURN
			SWITCH(
			    TRUE(),
			    Change > 0.05, "#2E8540",
			    Change < -0.05, "#D54309",
