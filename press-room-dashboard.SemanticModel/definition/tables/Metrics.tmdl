	/// Maximum daily views for a press release (peak performance).
	measure 'Peak Day Views' =
			VAR SelectedRelease = SELECTEDVALUE(Dim_Press_Releases[Page_URL])
			RETURN
			IF(
				NOT ISBLANK(SelectedRelease),
				CALCULATE(
					MAXX(
						VALUES(Fact_Press_Analytics[Date]),
						CALCULATE(
							SUM(Fact_Press_Analytics[Views]),
							Fact_Press_Analytics[Page_URL] = SelectedRelease,
							Fact_Press_Analytics[Page_Type] = "Press Release"
						)
					)
				),
				BLANK()
			)
			formatString: #,0
			displayFolder: 11 - Velocity & Lifecycle
			lineageTag: pr-m-peakdayviews
		lineageTag: accb0117-a2ba-4fb5-892b-96c83e0ec874

	/// Days until daily views drop below 50% of peak (half-life proxy).
	measure 'Half-life Proxy (Days)' =
			VAR SelectedRelease = SELECTEDVALUE(Dim_Press_Releases[Page_URL])
			VAR Peak = [Peak Day Views]
			VAR Threshold = Peak * 0.5
			VAR PubDate = SELECTEDVALUE(Dim_Press_Releases[Publish_Date])
			RETURN
			IF(
				NOT ISBLANK(SelectedRelease) && NOT ISBLANK(Peak) && Peak > 0,
				VAR DaysSincePub = [Days Since Publication]
				VAR DaysBelowThreshold =
					COUNTROWS(
						FILTER(
							VALUES(Fact_Press_Analytics[Date]),
							Fact_Press_Analytics[Date] >= PubDate &&
							CALCULATE(
								SUM(Fact_Press_Analytics[Views]),
								Fact_Press_Analytics[Page_URL] = SelectedRelease,
								Fact_Press_Analytics[Page_Type] = "Press Release"
							) < Threshold
						)
					)
				RETURN DaysSincePub - DaysBelowThreshold,
				BLANK()
			)
			formatString: #,0
			displayFolder: 11 - Velocity & Lifecycle
			lineageTag: pr-m-halflife
		lineageTag: 52ace345-5a32-48c8-8bea-35a63c2a5597

	/// Rising Stars ranking: Releases published in last 7-14 days ranked by velocity.
	measure 'Rising Stars Rank' =
			VAR SelectedRelease = SELECTEDVALUE(Dim_Press_Releases[Page_URL])
			VAR DaysSincePub = SELECTEDVALUE(Dim_Press_Releases[Days_Since_Publication])
			VAR IsRecent = DaysSincePub >= 7 && DaysSincePub <= 14
			RETURN
			IF(
				IsRecent && NOT ISBLANK(SelectedRelease),
				RANKX(
					FILTER(
						ALL(Dim_Press_Releases),
						Dim_Press_Releases[Days_Since_Publication] >= 7 &&
						Dim_Press_Releases[Days_Since_Publication] <= 14
					),
					[Views per Day Since Publication],
					,
					DESC,
					DENSE
				),
				BLANK()
			)
			formatString: #,0
			displayFolder: 11 - Velocity & Lifecycle
			lineageTag: pr-m-risingstarsrank
		lineageTag: 8bd9db0b-6b9a-4c75-bc95-73e6c349fad5

	/// Key Insight Signal: What changed? (e.g., "Organic views are up 18% vs prior 28 days")
	measure 'Key Insight - Signal' =
			VAR OrganicChange = [Organic Views Change % vs Prior 28]
			VAR ViewsChange = [Views Change % vs Prior 28]
			VAR UsersChange = [Users Change % vs Prior 28]
			VAR MaxChange = MAX(ABS(OrganicChange), MAX(ABS(ViewsChange), ABS(UsersChange)))
			RETURN
			SWITCH(
				TRUE(),
				ABS(OrganicChange) >= 0.15 && ABS(OrganicChange) = MaxChange,
					"Organic views are " &
					IF(OrganicChange > 0, "up ", "down ") &
					FORMAT(ABS(OrganicChange), "0%") & " vs prior 28 days",
				ABS(ViewsChange) >= 0.15 && ABS(ViewsChange) = MaxChange,
					"Total views are " &
					IF(ViewsChange > 0, "up ", "down ") &
					FORMAT(ABS(ViewsChange), "0%") & " vs prior 28 days",
				ABS(UsersChange) >= 0.15 && ABS(UsersChange) = MaxChange,
					"Total users are " &
					IF(UsersChange > 0, "up ", "down ") &
					FORMAT(ABS(UsersChange), "0%") & " vs prior 28 days",
				"Metrics are stable vs prior 28 days"
			)
			formatString: @
			displayFolder: 12 - Key Insights
			lineageTag: pr-m-keyinsight-signal
		lineageTag: a5632e9f-f688-4af6-bb62-d916b8522284

	/// Organic views change percentage vs prior 28 days (for Key Insights).
	measure 'Organic Views Change % vs Prior 28' =
			VAR OrganicLast28 =
				CALCULATE(
					[Organic Search Views],
					DATESINPERIOD(
						Dim_Date[Date],
						MAX(Dim_Date[Date]),
						-28,
						DAY
					)
				)
			VAR OrganicPrior28 =
				CALCULATE(
					[Organic Search Views],
					DATESINPERIOD(
						Dim_Date[Date],
						MAX(Dim_Date[Date]) - 28,
						-28,
						DAY
					)
				)
			RETURN
			DIVIDE(OrganicLast28 - OrganicPrior28, OrganicPrior28, 0)
			formatString: 0.0%
			displayFolder: 10 - Performance Context
			lineageTag: pr-m-organicchange28
		lineageTag: 3f88eb2f-15ea-4ce9-8756-b547141185de

	/// Key Insight Driver: Why? (e.g., "Driven by 3 releases (A, B, C) and higher SEO share")
	measure 'Key Insight - Driver' =
			VAR TopReleasesCount =
				CALCULATE(
					DISTINCTCOUNT(Dim_Press_Releases[Page_URL]),
					FILTER(
						VALUES(Dim_Press_Releases[Page_URL]),
						[Press Release Views] >
							CALCULATE(
								PERCENTILE.INC([Press Release Views], 0.75),
								ALL(Dim_Press_Releases)
							)
					)
				)
			VAR OrganicChange = [Organic Views Change % vs Prior 28]
			RETURN
			IF(
				TopReleasesCount > 0 && ABS(OrganicChange) >= 0.15,
				"Driven by " & TopReleasesCount & " high-performing releases and " &
				IF(OrganicChange > 0, "higher ", "lower ") & "SEO share",
				IF(
					TopReleasesCount > 0,
					"Driven by " & TopReleasesCount & " high-performing releases",
					"Driven by overall traffic trends"
				)
			)
			formatString: @
			displayFolder: 12 - Key Insights
			lineageTag: pr-m-keyinsight-driver
		lineageTag: eb73f7fc-a1fd-4e13-8efd-4dd4ef5f23a9

	/// Key Insight Action: What should we do? (e.g., "Republish/update related evergreen pages; promote via X channel")
	measure 'Key Insight - Action' =
			VAR OrganicChange = [Organic Views Change % vs Prior 28]
			VAR ViewsChange = [Views Change % vs Prior 28]
			VAR TopChannel =
				CALCULATE(
					SELECTEDVALUE(Fact_Press_Analytics[Channel_Group]),
					TOP1(
						VALUES(Fact_Press_Analytics[Channel_Group]),
						CALCULATE(SUM(Fact_Press_Analytics[Views])),
						DESC
					)
				)
			RETURN
			SWITCH(
				TRUE(),
				OrganicChange > 0.15,
					"Republish/update related evergreen pages; promote via " &
					IF(ISBLANK(TopChannel), "top channels", TopChannel),
				OrganicChange < -0.15,
					"Review SEO strategy; consider additional organic promotion",
				ViewsChange > 0.15,
					"Maintain current promotion strategy; focus on top-performing content",
				ViewsChange < -0.15,
					"Investigate traffic decline; review recent content and promotion",
				"Continue monitoring trends"
			)
			formatString: @
			displayFolder: 12 - Key Insights
			lineageTag: pr-m-keyinsight-action
		lineageTag: a9db7159-c9ed-45c4-9898-72d9c6221a59

