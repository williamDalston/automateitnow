	/// Quadrant categorization: Breakouts, Evergreen Winners, Needs Help, Underperformers.
	measure Quadrant =
			VAR V = [Velocity Score]
			VAR Vol = [Volume Score]
			VAR VMedian = [Median Velocity]
			VAR VolMedian = [Median Volume]
			RETURN
			SWITCH(
				TRUE(),
				NOT ISBLANK(V) && NOT ISBLANK(Vol) &&
				V >= VMedian && Vol >= VolMedian, "Breakouts",
				NOT ISBLANK(V) && NOT ISBLANK(Vol) &&
				V < VMedian && Vol >= VolMedian, "Evergreen Winners",
				NOT ISBLANK(V) && NOT ISBLANK(Vol) &&
				V >= VMedian && Vol < VolMedian, "Needs Help",
				NOT ISBLANK(V) && NOT ISBLANK(Vol), "Underperformers",
				BLANK()
			)
			formatString: @
			displayFolder: 13 - Quadrant Analysis
			lineageTag: pr-m-quadrant
		lineageTag: 48cab632-1432-46b4-b688-4c85d8f07e6d

